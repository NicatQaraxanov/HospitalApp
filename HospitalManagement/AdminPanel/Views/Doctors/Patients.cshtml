@model IEnumerable<Patient>
@{
    ViewData["Title"] = "Patients";
}


<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-white">Patients</h2>
		</div>
        <div class="col-6 text-end">
            <a asp-controller="Doctors" asp-action="Index" class="btn btn-primary">
               <i class="bi bi-journal-arrow-down"></i> &nbsp; Back To List
			</a>
		</div>
	</div>
    <br /><br />

    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Name
			    </th>
                <th>
                    Surname
			    </th>
                <th>
                    Age
			    </th>
		    </tr>
	    </thead>
        <tbody>
            @foreach(var obj in Model)
            {
                <tr>
                    <td>
                        @obj.Name
				    </td>
                    <td>
                        @obj.Surname
				    </td>
                    <td>
                        @obj.Age
				    </td>
			    </tr>
            }
        </tbody>

    </table>
</div>